<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.dotink.bean">

	<class name="Community" table="community">
		<id name="community_id">
			<generator class="native"></generator>
		</id>
		<property name="community_name"></property>
		<property name="community_code"></property>
		<property name="principal"></property>
		<property name="founder"></property>
		<property name="unit"></property>
		<property name="founddate"></property>
		<property name="quale"></property>
		<property name="intro"></property>
		<property name="money"></property>
		<property name="community_money"></property>
		<property name="note"></property>
		<property name="state"></property>
		<property name="user_account"></property>
	</class>

</hibernate-mapping>